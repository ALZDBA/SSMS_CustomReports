  <DataSets>
    <DataSet Name="Backups">
      <Query>
        <DataSourceName>Backups</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@db">
            <Value>=Parameters!DatabaseName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>set transaction isolation level read uncommitted
set lock_timeout 10000
/* geen msdb beschikbaar voor Azure SQLDb ! 
select  
(select max(BS.backup_finish_date) from msdb.dbo.backupset BS inner join sys.databases D on BS.database_name = D.physical_database_name
where BS.type = 'D' and D.name = @db) as [LastDatabaseBackup],
(select max(BS.backup_finish_date) from msdb.dbo.backupset BS inner join sys.databases D on BS.database_name = D.physical_database_name
where BS.type = 'I' and D.name = @db) as [LastDifferentialBackup],
(select max(BS.backup_finish_date) from msdb.dbo.backupset BS inner join sys.databases D on BS.database_name = D.physical_database_name
where BS.type = 'L' and D.name = @db) as [LastLogBackup]
*/
Select NULL as [LastDatabaseBackup], NULL as [LastDifferentialBackup], NULL as [LastLogBackup]
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="LastDatabaseBackup">
          <DataField>LastDatabaseBackup</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LastDifferentialBackup">
          <DataField>LastDifferentialBackup</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LastLogBackup">
          <DataField>LastLogBackup</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="FileSize">
      <Query>
        <DataSourceName>Backups</DataSourceName>
        <CommandText>set transaction isolation level read uncommitted

/* data file(s) info */
SELECT case DF.[type] when 0 then 'DATA'
	   when 1 then 'Log'
	   when 2 then 'FileStream'
	   When 3 then 'InternalUsageOnly'
	   When 4 then 'FullText(preSQL2014)'
	   Else '---UNDOCUMENTED---'
	   end AS [Type]
	, DS.name + ' - ' + DF.name as name
	, case when DFSU.total_page_count is null then DF.size * 8 / 1024 else DFSU.total_page_count * 8 / 1024 end AS [TotalMB]
	/*, case when DFSU.allocated_extent_page_count is null then DF.size * 8 / 1024 else DFSU.allocated_extent_page_count * 8 / 1024 end AS [UsedMB]*/
	, case when DFSU.user_object_reserved_page_count is null then DF.size * 8 / 1024 else DFSU.user_object_reserved_page_count * 8 / 1024 end AS [UsedMB]
FROM sys.database_files DF
inner join sys.data_spaces DS
	   on DS.data_space_id = DF.data_space_id
LEFT JOIN sys.dm_db_file_space_usage DFSU
	   ON DFSU.file_id = DF.file_id
where DF.[type] != 1 

UNION ALL
/* IMOLTP info */
SELECT 'InMemory_OLTP' AS [Type]
     , 'IMOLTP' AS Name
     , sum(ceiling( pages_kb / 1024.0 )) AS [TotalMB]
     , sum(ceiling( pages_kb / 1024.0 )) AS [UsedMB]
  FROM sys.dm_os_memory_clerks
  WHERE type LIKE '%xtp%'
  and case when name = 'Client-Default' then 0
		  when SUBSTRING(Name, 7,DATALENGTH(name)) = db_id() then 1
	   else 0
	   end = 1
  having sum(ceiling( pages_kb / 1024.0 ))  &gt; 0

union all

/* Log info */
SELECT 'Log' AS [Type]
	, 'Log - ' + DF.name
	, case DF.file_id when 2 then L.LogTotalMB else 1 end as LogTotalMB
	, case DF.file_id when 2 then L.LogUsedMB else 0 end as LogUsedMB

FROM( SELECT DbName AS DatabaseName
		  , [LOG File(s) Size (KB)] / 1024 AS LogTotalMB
		  , [Log File(s) Used Size (KB)] / 1024 AS LogUsedMB
		  FROM(
			 SELECT DB.name as DbName, OPC.*
				FROM sys.dm_os_performance_counters OPC
				INNER JOIN sys.databases DB 
					on DB.physical_database_name = OPC.instance_name
				WHERE OPC.counter_name IN( 'Log File(s) Size (KB)', 'Log File(s) Used Size (KB)' )
				    AND OPC.instance_name != '_Total'
				    AND DB.name = DB_NAME( )
			 )AS Src 
		PIVOT( MAX( cntr_value )
			 FOR counter_name IN( [LOG File(s) Size (KB)], [Log File(s) Used Size (KB)] )
			 )AS pvt 
		  )AS L
	   INNER JOIN sys.database_files DF
		  ON DF.[type] = 1 /* Log */

order by [Type],[Name];
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Type">
          <DataField>Type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalMB">
          <DataField>TotalMB</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="UsedMB">
          <DataField>UsedMB</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="FreeMB">
          <Value>=Fields!TotalMB.Value-Fields!UsedMB.Value</Value>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DatabaseProperties">
      <Query>
        <DataSourceName>Backups</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@db">
            <Value>=Parameters!DatabaseName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>set transaction isolation level read uncommitted
set lock_timeout 10000

select d.owner_sid as [Owner],d.create_date as [crdate],d.compatibility_level as [cmptlevel]
,d.collation_name as [Collation],d.user_access_desc as [UserAccess],
d.state_desc as [Status],d.recovery_model_desc as [Recovery]
from sys.databases d
where d.[name] = @db</CommandText>
